doctype 5
-var location = test2[0][10]
-var zipcode = test2[0][11]

html
  head
    style body {font-family:arial} p {font-size:15px} tr:nth-child(2n-1) {background-color:lightgray} 
  body
    h2(style="color:#003399") 5 Day Weather Predictions for #{location} (#{zipcode})
    table
        tbody(align="center")
            tr
                td(style="padding:5px;") Time
                td(style="padding:5px;") Temperature
                td(style="padding:5px;") Chance of Precipitation
                td(style="padding:5px;") Precipitation Type
                td(style="padding:5px;") Cloud Coverage
            each val in test2
                tr
                    td(style="padding:5px;")=val[4]
                    td(style="padding:5px;")=val[0]
                    td(style="padding:5px;")=val[1]
                    td(style="padding:5px;")=val[2]
                    td(style="padding:5px;")=val[3]
                tr
                    td(style="padding:5px;")=val[9]
                    td(style="padding:5px;")=val[5]
                    td(style="padding:5px;")=val[6]
                    td(style="padding:5px;")=val[7]
                    td(style="padding:5px;")=val[8]